{
    "name": "eslint-rules-zh-plugin",
    "displayName": "ESLint Chinese Rules",
    "description": "eslint中文规则提示插件",
    "version": "0.2.2",
    "publisher": "maggie",
    "author": {
        "name": "maggie"
    },
    "keywords": [
        "eslint",
        "rules",
        "chinese",
        "zh"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/maggie-chen/eslint-rules-zh-plugin.git"
    },
    "bugs": {
        "url": "https://github.com/maggie-chen/eslint-rules-zh-plugin/issues",
        "email": "maggie_jsj@163.com"
    },
    "homepage": "https://github.com/maggie-chen/eslint-rules-zh-plugin/blob/master/README.md",
    "license": "MIT",
    "icon": "assets/icon.png",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript",
        "onLanguage:javascriptreact",
        "onLanguage:typescript",
        "onLanguage:typescriptreact",
        "onLanguage:vue"
    ],
    "main": "./src/extension.js",
    "extensionPack": [
        "dbaeumer.vscode-eslint"
    ],
    "contributes": {
        "configuration": {
            "title": "ESLint Chinese Rules ( eslint中文规则提示插件 )",
            "properties": {
                "eslint-rules-zh-plugin.enableErrorLens": {
                    "type": "boolean",
                    "default": true,
                    "description": "是否开启行尾错误提示"
                }
            }
        }
    },
    "scripts": {},
    "devDependencies": {
        "@types/vscode": "^1.50.0",
        "eslint": "^7.9.0"
    }
}
